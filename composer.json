{
  "name": "bcolemutech/wp-vouched-verify",
  "description": "WordPress plugin using Vouched to validate users and ensure users have a single account.",
  "type": "wordpress-plugin",
  "license": "GPL-2.0-or-later",
  "require-dev": {
    "ext-curl": "*",
    "ext-dom": "*",
    "phpunit/phpunit": "9.*",
    "johnpbloch/wordpress": "5.*"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Test\\": "test"
    }
  },
  "require": {
    "ext-json": "*"
  }
}
